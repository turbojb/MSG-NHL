  #form-wrapper {
    font-family: 'TradeGothicLTStdBdCnNo.20' !important;
    margin-top: 156px;
    margin-left: 220px; 
    padding: 0px;
    width: 368px;
    height: 471px; 
    overflow:hidden;
    background-image:url(<%= asset_path 'form-bg.png' %>);
    background-repeat:no-repeat;
    float:left; 

    .form-header {
      font-size: 26px;
      color: #FFFFFF;
      margin: 10px 0 0 10px;
    }

    b.large_word {
      font-size: 12px;
      color: #FFFFFF;
    }

    p.header_copy {
      font-family: "arial";
      font-size: 12px;
      color: #FFFFFF;
      margin: 10px 0 0 10px;
      width: 325px;
      line-height: 15px;
    }

    #our_entry_form {
      font-family: 'TradeGothicLTStdBdCnNo.20' !important;
      font-weight: 10px;
      margin: 10px 0 0 15px ;
      color: #FFFFFF;


      .field {
        font-family: "arial";
        margin: 4px 0 0px 0;
        height: 30px;
      }

      .dropdown {
        margin: 0 0 0 0;
      }

      label {
        float: left;
        width: 120px;
        font-size: 14px;
        color: #ffffff;
        line-height: 22px;
      }

      label.small-label{

        float: left;
        width: 120px;
        font-size: 11px;
        color: white;
        line-height: 22px;
      }

      input {
        width: 200px;
        line-height: 12px;
        height: 12px;
        background-color:#FFF;
        padding: 4px;
        border: none;
        float: left;
        font-size: 12px;
        color:#000;

        -moz-box-shadow:    inset 2px 2px 6px #666;
        -webkit-box-shadow: inset 2px 2px 6px #666;
        box-shadow:         inset 2px 2px 6px #666;
      }

      .zip_code {
        width: 200px;
      }


      .checkboxes {
        color: #FFF;
        margin: 0 0 0 -5px;
        width: 300px;


        input[type=checkbox] {
          float: left;
          background-color:transparent;
          width:14px;
          height: 14px;
        }

        .chx {
          width:20px;
          height: 20px;
          float: left;
        }

        label  {
          font-size: 10px;
          float: left;
          width: 241px;
          margin: 2px 0 8px 6px;
          line-height: 14px;

          a {
            color: #000;
            border-bottom: 1px #bebebe solid;
            &:hover {
              border-bottom: 1px #333 solid;                
            }
          }
        }

        .checkbox {
          margin: 0 0 0 0;
          clear: both;
          height: 18px;
        }

      }

      .actions  {
        float: left;
        width: 325px;
        margin: 0 0 0 0;

        .submit_btn {
          display:block;
          width: 105px;
          height: 37px;
          text-indent: -3000px;
          border: none;
          background: url(<%= asset_path 'enter-but.png' %>) no-repeat 0px 0px;
          cursor: pointer;
          margin: 0 0 0 0;
          float: right;
          -moz-box-shadow:    none;
          -webkit-box-shadow: none;
          box-shadow:         none;
          

          &:hover {
            background-position: 0px -0px;
          }
          &:active {
            background-position: 0px -39px;
          }            

        }

      }

      .required_denote {
        font-size: 11px;
        color: #e9b628;
        font-style: italic;
        margin: 16px 0 0 0;
        float: left;
      }

    }
  }


